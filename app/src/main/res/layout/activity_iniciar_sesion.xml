<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/c"
    tools:context=".IniciarSesion">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="333dp"
        android:layout_height="0dp"
        android:layout_marginBottom="114dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.731">

        <EditText
            android:id="@+id/et_email"
            android:layout_width="match_parent"
            android:layout_height="57dp"
            android:background="#FF6D00"
            android:backgroundTint="#AEEA00"
            android:hint="Ingrese Email"
            android:inputType="textEmailAddress"
            android:textSize="24sp" />

        <EditText
            android:id="@+id/et_password"
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:background="#B564DD17"
            android:backgroundTint="#64DD17"
            android:hint="Ingrese password"
            android:inputType="textPassword"
            android:textSize="24sp" />

        <Button
            android:id="@+id/btn_sesion"
            android:layout_width="match_parent"
            android:layout_height="64dp"
            android:backgroundTint="#00B8D4"
            android:fontFamily="sans-serif-black"
            android:onClick="IniciarSesion"
            android:text="Iniciar sesion"
            android:textSize="24sp"
            app:strokeColor="#FFD600" />
    </LinearLayout>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="58dp"
        android:layout_marginBottom="56dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/k" />

</androidx.constraintlayout.widget.ConstraintLayout>